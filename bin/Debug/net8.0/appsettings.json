{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Confluent.Kafka": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "webhook-events",
    "Settings": {
      "MessageTimeoutMs": 30000,
      "RequestTimeoutMs": 20000,
      "SocketTimeoutMs": 20000,
      "EnableDeliveryReports": true
    }
  }
}